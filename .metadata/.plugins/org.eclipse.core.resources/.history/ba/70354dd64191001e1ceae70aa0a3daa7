
import java.util.Locale;
import java.util.Scanner;

public class Main {
	public static void main(String[] args) {
		Locale.setDefault(Locale.US);
		Scanner sc = new Scanner(System.in);

		 double numero = sc.nextDouble();
		 double total = 0;
		 
		 for (int i = 0; i < 3; i++) {
			total = total + numero;
		}
		
		 System.out.println(total);
		
		

	}

}