
import java.util.Locale;
import java.util.Scanner;

public class Main {
	public static void main(String[] args) {
		Locale.setDefault(Locale.US);
		Scanner sc = new Scanner(System.in);
		
		 double x = 3;
		 double y = 0;
		 
		 for(int i=0; i<x; i++) {
			 System.out.print(i + ", ");
			 y = y + 5;
			 System.out.println(y);
			 
		 }
		

		 sc.close();
			

	}

}